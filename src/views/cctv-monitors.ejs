          <script type="text/javascript">
            let buttonU = document.getElementById('btnUHD');
            let buttonF = document.getElementById('btnFHD');
            let buttonH = document.getElementById('btnHD');

            let xU = buttonU.nextElementSibling;
            let xF = buttonF.nextElementSibling;
            let xH = buttonH.nextElementSibling;

            let showHideU = document.getElementById("show-hideUHD");
            let showHideF = document.getElementById("show-hideFHD");
            let showHideH = document.getElementById("show-hideHD");

            showHideU.textContent = "show more.."
            showHideF.textContent = "show more.."
            showHideH.textContent = "show more.."

            xU.style.display = 'none';
            xF.style.display = 'none';
            xH.style.display = 'none';


            function showFeaturesUHD() {
              if (xU.style.display === 'none') {
                xU.style.display = 'block';
                xU.classList.add("animated-content");
                showHideU.textContent = "hide"
              } else {
                xU.style.display = 'none';
                xU.classList.remove("animated-content");
                showHideU.textContent = "show more.."
              }
            }

            function showFeaturesFHD() {
              if (xF.style.display === 'none') {
                xF.style.display = 'block';
                xF.classList.add("animated-content");
                showHideF.textContent = "hide"
              } else {
                xF.style.display = 'none';
