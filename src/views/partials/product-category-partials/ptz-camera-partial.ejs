<div id="cameras" class="product-card-holder cctv-ptz" data-ix="fade-on-scroll-2">
  <% data.forEach(function(el){ %>
  <div class="individual-product product-card" id="main-categoriesId">
    <a href="<%= `/products/cctv/cameras/${el.product_code}` %>">
      <div class="product-image">
        <img class="product-list-image" src="<%=`${el.image}` %>" alt="">
      </div>
      <br>
      <%= el.product_name %>
      <br> <br>
      <span class="product_code">
        <%= el.product_code %>
      </span>
    </a>
  </div>
  <% }) %>
</div>